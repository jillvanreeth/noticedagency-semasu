    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}{% endblock %} | Semasu</title>
    <meta name="description" content="Semasu">
    <meta name="viewport" content="width=device-width, initial-scale=1">

